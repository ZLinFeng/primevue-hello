{
    "eslint.validate": [
        "javascript",
        "vue",
        "vue-html",
        "typescript",
        "typescriptreact",
        "html",
        "css",
        "scss",
        "less",
        "json",
        "jsonc",
        "json5",
        "markdown"
    ],
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "always"
    },
    "editor.tabSize": 4,
    "files.associations": {
        "*.css": "tailwindcss"
    }
}